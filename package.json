{
  "name": "bayhan-miniapp",
  "private": true,
  "packageManager": "pnpm@5.85.5",
  "@tanstack/react-query": "^5.85.5",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev:web": "pnpm -C apps/web dev",
    "build:web": "pnpm -C apps/web build",
    "start:web": "pnpm -C apps/web start",
    "db:generate": "pnpm -C apps/web prisma generate",
    "db:migrate": "pnpm -C apps/web prisma migrate dev",
    "db:seed": "pnpm -C apps/web tsx prisma/seed.ts",
    "dev:bot": "pnpm -C apps/bot dev",
    "start:bot": "pnpm -C apps/bot start"
  }
}
